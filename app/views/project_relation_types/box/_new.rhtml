<% form_for :project_relation_type, :url=>{:action=>:create},:class=>"tabular" do |f| %>
  <%= f.error_messages %>
  <p>
    <label><%= l(:label_project_relation_type) %></label>
    <%= f.text_field :relation_type %>
  </p>
  <div class="buttons right">
    <%= submit_tag l(:button_save),:class=>"btn_blue button corner-all" %>
  </div>
<% end %>