<% unless photos.nil?  %>
  <% photos.each do |photo| %>
    <div class="left">
      <div class="photo_item editable_box">
        <div class="links_edit_box">
          <%= link_to_remote("#{image_tag('/images/delete.png')}",{:url=>{:controller=>:photos,:action=>:destroy,:id=>photo.id,:project_id=>photo.gallery.project,:gallery_id=>photo.gallery.id},:method=>:get,:confirm=>'Etes-vous sÃ»r ?'},:class=>"links_edit_box")  %>
        </div>
       <%= image_tag photo.photo.url(:thumb) %>
      </div>
    </div>
  <% end %>
<div class="clearer"></div>
<% end %>