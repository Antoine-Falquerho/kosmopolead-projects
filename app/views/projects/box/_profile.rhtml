<div class="project_infos splitcontentright">
  <% if  project.author %>
    <%= l(:label_support_by) %> <%= project.author.name %><br/>
  <% end %>
  <% if  project.watcher %>
    <%= l(:label_watched_by) %> <%= project.watcher.name %><br/>
  <% end %>
  <% if  project.builder %>
    <%= l(:label_build_by) %> <%= project.builder.name %><br/>
  <% end %>
</div>
<div class="clearer"></div>